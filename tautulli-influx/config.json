{
  "name": "Tautulli infludb",
  "version": "dev",
  "slug": "tautulli-influx",
  "description": "Export Tautulli metrics directly to InfluxDb",
  "url": "https://github.com/Sabuto/tautulli-influxdb-export",
  "ingress": false,
  "startup": "services",
  "homeassistant": "0.92.0b2",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "map": [
    "share:rw",
    "ssl"
  ],
  "boot": "auto",
  "ports": {},
  "ports_description": {},
  "hassio_api": true,
  "auth_api": true,
  "hassio_role": "default",
  "homeassistant_api": false,
  "options": {
    "interval": 5,
    "host": "a0d7b954_tautulli",
    "port": 8181,
    "tautulliapikey": "",
    "tautullibaseurl": "",
    "influxdbhost": "a0d7b954_influxdb",
    "influxdbport": 8086,
    "influxdbuser": "tautulli",
    "influxdbpw": "",
    "influxdbdatabase": "tautulli",
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "interval": "int",
    "host": "str",
    "port": "int",
    "tautulliapikey": "str",
    "tautullibaseurl": "str",
    "influxdbhost": "str",
    "influxdbport": "int",
    "influxdbuser": "str",
    "influxdbpw": "str",
    "influxdbdatabase": "str",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  }
}
